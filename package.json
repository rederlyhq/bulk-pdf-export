{
  "name": "bulk_pdf_export",
  "version": "0.0.5",
  "description": "A microservice that expects JSON information to put into a PDF.",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node src/app-scripts/index.ts",
    "start:watch": "nodemon src/app-scripts/index.ts",
    "start:debug": "node --inspect=5857 -r ts-node/register src/app-scripts/index.ts",
    "start:debug:watch": "nodemon --inspect=5857 src/app-scripts/index.ts",
    "start:docker": "docker run --add-host host.docker.internal:host-gateway --cap-add=SYS_ADMIN -p 3005:3005 --rm -it $(docker build -q .)",
    "build": "npm run clean && tsc",
    "clean": "if [ -d ts-built ]; then rm -r ts-built; fi;",
    "lint": "eslint src/ --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Gibryon Bhojraj",
  "license": "GPL-3.0",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts",
    "execMap": {
      "ts": "node --require ts-node/register"
    },
    "signal": "SIGINT"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.3.0",
    "@aws-sdk/lib-storage": "3.3.0",
    "archiver": "^5.2.0",
    "async-mutex": "^0.2.6",
    "axios": "^0.21.1",
    "boom": "^7.3.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "pdfjs-dist": "^2.6.347",
    "pug": "^3.0.0",
    "puppeteer-core": "^5.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/archiver": "^5.1.0",
    "@types/boom": "^7.3.0",
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.167",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/pug": "^2.0.4",
    "@types/puppeteer": "^5.4.2",
    "@types/puppeteer-core": "^5.4.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "acorn": "^8.0.4",
    "eslint": "^7.17.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
